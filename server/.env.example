# Claude API Key for food image analysis
# Sign up at https://console.anthropic.com/ to get your API key
CLAUDE_API_KEY=your_claude_api_key_here

# Server Port (default: 3002) - Using SERVER_PORT instead of PORT to avoid Firebase reserved names
SERVER_PORT=3002

# Development mode - set to "true" to bypass Firebase auth and use mock data
DEV_MODE=false

# Firebase Admin SDK configuration - renamed to avoid Firebase reserved prefixes
PROJECT_ID=your_firebase_project_id
CLIENT_EMAIL=your_service_account_email
PRIVATE_KEY="your_private_key_with_newlines" 